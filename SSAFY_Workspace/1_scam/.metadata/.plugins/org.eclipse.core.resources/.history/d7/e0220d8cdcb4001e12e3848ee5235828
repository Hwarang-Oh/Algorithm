package com.ssafy.day3.a_inheritance;

public class Person {
	String name;
	
	public Person(String name) { // 이걸 해버려서, SpiderMan의 생성자가 영향이 있음. 
		super(); // 항상 생략되어 있는, this(); super(); 없으면 super(); 삽입해줌.
		this.name = name;
	}
	
	void eat() {
		System.out.println("냠냠");
	}
	void jump() {
		System.out.println("폴짝");
	}
}
