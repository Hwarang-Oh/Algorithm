import java.util.Scanner;
import java.util.stream.IntStream;

public class LastDayTest {

	public static void main(String[] args) {
		// 년, 월 입력 받아 해당 월의 마지막 날짜를 구하시오
		// 반복적으로 입력 받아 처리
		Scanner scan = new Scanner(System.in);
		System.out.print("Year Month : ");
		int year;
		int month;
		year = scan.nextInt();
		month = scan.nextInt(); 
		int [] day_31 = {1,3,5,7,8,10,12};
		int [] day_30 = {4,6,9,11};
		
		while (true) {
			if (IntStream.of(day_31).anyMatch(x -> x == month))
				System.out.println("31일");
			if (IntStream.of(day_30).anyMatch(x -> x == month))
				System.out.println("30일");
			
			boolean isLeap;
			if (year % 4 == 0) {
				if (year % 400 == 0) {
					isLeap = true;
				}
				else if (year % 100 == 0) {
					isLeap = false;
				}
				else 
					isLeap = true;
			}
			else 
				isLeap = false;
			
			if (month == 2){
				if (isLeap == true)
					System.out.println("28일");
				else 
					System.out.println("29일");	
			}
			year = scan.nextInt();
			month = scan.nextInt(); 
		}
	}
}
