import java.util.Scanner;

public class LastDayTest {

	public static void main(String[] args) {
		// 년, 월 입력 받아 해당 월의 마지막 날짜를 구하시오
		// 반복적으로 입력 받아 처리
		Scanner scan = new Scanner(System.in);
		int year = 0;
		int month = 0;
		
		while (year == -1) {
			year = scan.nextInt();
			month = scan.nextInt(); 
			boolean isLeap;
	
			if (year % 4 == 0) {
			
				if (year % 400 == 0) {
					isLeap = true;
				}
				else if (year % 100 == 0) {
					isLeap = false;
				}
				else 
					isLeap = true;
			}
			else 
				isLeap = false;
		}
		
		System.out.println("ㅋㅋㅋ");
	}
}
