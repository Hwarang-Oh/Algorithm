package com.ssafy.day1.b_array;

public class ArrayTest_05 {
    public static void main(String[] args) {
        String org = "1234567890";
        char[] nums = org.toCharArray();
        int sum = 0;
        
        for (int i = 0 ; i < nums.length ; i++) {
        	sum += nums[i] - '0';
        }

        // END
    }
}
